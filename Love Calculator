print("Welcome to the Love Calculator!")
name1 = input("What is your name? \n")
name2 = input("What is their name? \n")






low_name1= name1.lower()
low_name2 = name2.lower()

combined_low_names = low_name1 + low_name2

t = combined_low_names.count("t")
r = combined_low_names.count("r")
u = combined_low_names.count("u")
e = combined_low_names.count("e")

x= t + r + u + e

l = combined_low_names.count("l")
o = combined_low_names.count("o")
v = combined_low_names.count("v")
e = combined_low_names.count("e")

y = l + o + v + e

score = int(str(x)+ str(y))

if (score < 10) or (score > 90):
 print(f" Your score is {score}, you go together like coke and mentos.")

elif (score>=40) and (score<=50):
 print(f"Your score is {score}, you are alright together.")

else:   
    print(f"Your score is {score}")
